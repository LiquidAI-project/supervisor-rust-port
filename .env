

PUBLIC_HOST="http://127.0.0.1" # Orchestrator address
PUBLIC_PORT="3000"  # Orchestrator port
WASMIOT_ORCHESTRATOR_URL=${PUBLIC_HOST}:${PUBLIC_PORT}
DEFAULT_CAMERA_DEVICE=0 # Device number for capturing images with opencv
WASMIOT_LOGGING_ENDPOINT=${PUBLIC_HOST}:${PUBLIC_PORT}/device/logs
RUST_LOG=info # Sets the default logging level (supervisor only, doesnt affect logs sent to orchestrator)
EXTERNAL_LOGGING_ENABLED="true"
SUPERVISOR_NAME=rust-port
DEVICE_TYPE=RaspberryPi-with-camera
WASMIOT_SUPERVISOR_IP="127.0.0.1"
WASMIOT_SUPERVISOR_PORT="8080"
DEFAULT_URL_SCHEME="http"
WASM_RUNTIME="wasmtime"
MINIMAL_IMPLEMENTATION="false"

